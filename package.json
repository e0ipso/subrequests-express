{
  "name": "subrequests-express",
  "version": "0.0.0-development",
  "description": "Integrates Subrequests in your Express project",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul check-coverage --statements 100 --lines 100 --branches 100 --functions 100 **/coverage.json",
    "test": "npm run build && istanbul cover --print both nodeunit ./test",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run flow && npm run lint",
    "prepush": "npm test && npm run coverage",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "postinstall": "npm run build",
    "flow": "flow",
    "build": "flow-remove-types src/ --out-dir lib/ && flow-copy-source src lib",
    "lint": "eslint src"
  },
  "keywords": [
    "subrequests",
    "express",
    "http"
  ],
  "author": "Mateu Aguil√≥ Bosch <mateu.aguilo.bosch@gmail.com> (e0ipso)",
  "license": "GPL-2.0",
  "peerDependencies": {
    "express": "^4.14.0",
    "subrequests": "^1.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "nodeunit": "^0.11.1",
    "semantic-release": "^7.0.2",
    "semantic-release-conventional-commits": "^1.0.2",
    "sinon": "^3.0.0",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "config": "^1.26.1",
    "coveralls": "^2.13.1",
    "flow-copy-source": "^1.2.0",
    "flow-remove-types": "^1.2.1"
  },
  "release": {
    "analyzeCommits": "semantic-release-conventional-commits"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e0ipso/subrequests-express.git"
  }
}
